<div class="w-full">
  <%= render partial: 'notice' %>

  <h1 class="font-bold text-xl mb-6 pb-6 border-b-2">
    Your Pragmatic Catalog
  </h1>

  <%= turbo_stream_from 'products' %>
  <ul>
    <% cache @products do %>
      <% @products.each do |product| %>
        <% cache product do %>
          <%= render partial: 'product', object: product %>
        <% end %>
      <% end %>
    <% end %>
  </ul>
</div>